@page
@model IndexModel
@{
    @section Cd{<p>Index页面的Section</p>}
    <h1>我是Index内容页</h1>
    @for (int i = 0; i < 5; i++)
    {
        for (int j = i; j < 5; j++)
        {
            <span>&nbsp;</span>
        }
        for (int z = 0; z <= 2 * i; z++)
        {
            if (i % 2 == 0)
            {
                <span style="color:blue">i</span>
            }
            else
            {
                <span style="color:red">i</span>
            }
        }
        <br />
    }

    <table class="table table-striped" id="month">
        <tbody>
            <tr>
                <th>一</th>
                <th>二</th>
                <th>三</th>
                <th>四</th>
                <th>五</th>
                <th>六</th>
                <th>日</th>
            </tr>
        </tbody>
    </table>

   
        
           
      
}

