@model  _17bangMVC.Models.Register.MessageModel

@{
    //ViewBag.Title = "Home Page";
}

<h1>消息！！！</h1>
@using (Html.BeginForm())
{
    for (int i = 0; i < Model.Messages.Count; i++)
    {
        <div>
            @Html.CheckBoxFor(m => Model.Messages[i].Selecteed)
            @Model.Messages[i].Content;
            @Html.HiddenFor(m=>Model.Messages[i].Id)
        </div>
    }
    <button type="submit">提交</button>
}

